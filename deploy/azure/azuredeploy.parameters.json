{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "namePrefix": {
      "value": "probotsharp"
    },
    "environment": {
      "value": "production"
    },
    "location": {
      "value": "eastus"
    },
    "appServicePlanSku": {
      "value": "B1"
    },
    "postgreSqlSku": {
      "value": "Standard_B1ms"
    },
    "githubAppId": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{resource-group}/providers/Microsoft.KeyVault/vaults/{keyvault-name}"
        },
        "secretName": "github-app-id"
      }
    },
    "githubWebhookSecret": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{resource-group}/providers/Microsoft.KeyVault/vaults/{keyvault-name}"
        },
        "secretName": "github-webhook-secret"
      }
    },
    "githubPrivateKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{resource-group}/providers/Microsoft.KeyVault/vaults/{keyvault-name}"
        },
        "secretName": "github-private-key"
      }
    },
    "postgresqlPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{resource-group}/providers/Microsoft.KeyVault/vaults/{keyvault-name}"
        },
        "secretName": "postgresql-password"
      }
    }
  }
}
